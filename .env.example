# Secan Environment Configuration
# Copy this to .env and customize for your environment

# Server Configuration
SECAN_SERVER__HOST=127.0.0.1
SECAN_SERVER__PORT=27182

# Authentication Configuration
SECAN_AUTH__MODE=open
SECAN_AUTH__SESSION_TIMEOUT_MINUTES=30

# Cluster Configuration (Local Development)
SECAN_CLUSTERS__0__ID=local
SECAN_CLUSTERS__0__NAME=Local Development
SECAN_CLUSTERS__0__NODES__0=http://localhost:9200
SECAN_CLUSTERS__0__ES_VERSION=8

# Logging Configuration
RUST_LOG=info
